<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
  <ui:style type="coreen.project.ModulesPanel.Styles" field="_styles">
    .modules {
      font-size: small;
      margin: 5px 0px;
    }
    .modules div {
      display: inline;
    }
    .modprefix {
      font-weight: bold;
    }
    .modlink {
      margin-left: 5px;
    }
    .moddefs {
      border-top: 1px solid;
      padding-top: 5px;
      margin-bottom: 3px;
    }
    .modtitle {
      font-weight: bold;
    }
    .type {
      margin-right: 5px;
    }
  </ui:style>

  <g:HTMLPanel>
    <g:FlowPanel styleName="{_styles.modules}" ui:field="_modules"/>
    <g:FlowPanel styleName="{_styles.moddefs}" ui:field="_moddefs"/>
    <g:FlowPanel ui:field="_types"/>
  </g:HTMLPanel>
</ui:UiBinder>
