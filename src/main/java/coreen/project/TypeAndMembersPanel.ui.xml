<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
  <ui:style type="coreen.project.TypeAndMembersPanel.Styles" field="_styles">
    .contents {
    }
    .topgap {
      border-top: 1px solid;
      margin-top: 2px;
    }
    .header {
    }
    .summaryMembers {
      margin-left: 20px;
    }
    .detailMembers {
      border-top: 1px dashed;
      border-bottom: 1px dashed;
    }
    .sigPanel {
      padding-bottom: 2px;
    }
    .member {
      border-top: 1px dashed;
    }
    .superUp {
      color: #BBBBBB;
    }
    .outerHovered {
      background: #FFCCCC;
    }
    .filterLabel {
      margin-left: 25px;
    }
    .filterLabel, .filterBox {
      font-size: small;
    }
    .filterBox {
      background: #FFFFFF;
      border: none;
      margin: none;
      vertical-align: top;
      margin-top: 3px;
    }
  </ui:style>

  <g:FlowPanel styleName="{_styles.contents}" ui:field="_contents"/>
</ui:UiBinder>
