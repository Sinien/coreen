<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
  <ui:style type="coreen.config.EditProjectPage.Styles" field="_styles">
    body {
      font-family: Helvetica, Arial, sans-serif;
    }
    .section {
      width: 100%;
      border-bottom: 1px solid;
      padding-bottom: 5px;
      margin-bottom: 5px;
    }
    .inline {
      display: inline;
    }
    .title {
      font-family: Trebuchet MS, serif;
      font-size: 24px;
    }
    .tip {
      font-size: small;
      max-width: 450px;
    }
  </ui:style>

  <g:HTMLPanel>
    <div class="{_styles.section} {_styles.title}">
      <g:Hyperlink targetHistoryToken="" styleName="{_styles.inline}">Projects</g:Hyperlink> - 
      <g:Hyperlink ui:field="_name" styleName="{_styles.inline}"/>
    </div>
    <g:SimplePanel ui:field="_contents"/>
  </g:HTMLPanel>
</ui:UiBinder>
