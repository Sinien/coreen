<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
  <ui:style>
    body {
      font-family: Helvetica, Arial, sans-serif;
    }
    .header {
      border-bottom: 1px solid;
      padding-bottom: 5px;
      width: 100%;
    }
    .title {
      font-family: Trebuchet MS, serif;
      font-size: 24px;
    }
  </ui:style>

  <g:HTMLPanel>
    <table class="{style.header}" cellpadding="0" cellspacing="0">
    <tr><td valign="top" class="{style.title}">
          <g:Hyperlink targetHistoryToken=""><ui:msg>Projects</ui:msg></g:Hyperlink></td>
        <td><ui:msg>Search:</ui:msg>
            <g:TextBox ui:field="_search" visibleLength="30"/>
            <g:Button ui:field="_go"><ui:msg>Go</ui:msg></g:Button></td>
        <td align="right" valign="bottom">
          <a href="#IMPORT"><ui:msg>Import...</ui:msg></a>
        </td>
    </tr>
    </table>

    <g:SimplePanel ui:field="_contents"/>
  </g:HTMLPanel>
</ui:UiBinder>
